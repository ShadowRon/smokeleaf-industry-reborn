<Defs>

	<!--========== Trim Smokeleaf ==========-->

  	<RecipeDef>
		<defName>AutoTrimSmokeleaf</defName>
		<label>auto trim smokeleaf</label>
		<description>Trim large amounts of smokeleaf very quickly. Due to the machine process, a small percentage of buds and seeds will be lost.</description>
		<jobString>Trimming Smokeleaf.</jobString>
		<effectWorking>Tailor</effectWorking>
		<soundWorking>Recipe_Tailor</soundWorking>
		<workAmount>1500</workAmount>
  		<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
		<workSkill>Crafting</workSkill>
		<workSkillLearnFactor>2.0</workSkillLearnFactor>
		<skillRequirements>
			<Crafting>5</Crafting>
		</skillRequirements>
		<researchPrerequisite>AdvancedSmokeleafCultivation</researchPrerequisite>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>SmokeleafLeaves</li>
					</thingDefs>
				</filter>
				<count>25</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>SmokeleafLeaves</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<HempFiber>25</HempFiber>
			<SmokeleafBuds>20</SmokeleafBuds>
			<SmokeleafSeed>20</SmokeleafSeed>
		</products>
		<recipeUsers>
			<li>SmokeleafAutoTrimmer</li>
		</recipeUsers>
	</RecipeDef>
	
